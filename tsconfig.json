{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["ES2021", "DOM"],
    "jsx": "react-jsx",
    "types": ["@cloudflare/workers-types"],
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": "."
  },
  "include": ["src/**/*", "frontend/**/*"],
  "exclude": ["node_modules", "frontend/dist"]
}

